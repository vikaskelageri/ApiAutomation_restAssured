<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <!-- Register both ExtentReporter and TestListener -->
    <listeners>
        <listener class-name="listeners.ExtentReporterNG"/> <!-- Existing ExtentReporter -->
    </listeners>
    
    <test thread-count="5" name="Test">
        <classes>
            <class name="testCases.LoginPageTest"/> <!-- Test class for LoginPage -->
            <class name="testCases.HomePageTest"/>  <!-- Test class for HomePage -->
        </classes>
    </test>
</suite>
