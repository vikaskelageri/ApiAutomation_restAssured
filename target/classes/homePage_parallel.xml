<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <!-- Register both ExtentReporter and TestListener -->
    <listeners>
        <listener class-name="listeners.ExtentReporterNG"/> <!-- Existing ExtentReporter -->
    </listeners>
    
    <test name="HomePageTestExecution" parallel="methods" thread-count="5">
        <classes>
            <class name="testCases.HomePageTest"/> <!-- Only run HomePageTest class -->
        </classes>
    </test>
</suite>
